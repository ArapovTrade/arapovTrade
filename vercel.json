{
  "routes": [
    {
      "src": "^/static/(.*)$",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "dest": "/static/$1"
    },
    {
      "src": "/(.*).(png|jpg|jpeg|gif|svg|webp|ico|css|js)$",
      "dest": "/$1.$2"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}